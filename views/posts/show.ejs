<!DOCTYPE html>
<html>
  <!-- ===========HEAD============= -->
    <%- include ('../partials/head.ejs') %>
  <!-- ============================ -->
    <body>
      <div class="header">
       <a href="/toolow" class="logo">TooLowForComfort</a>
       </div>
      <nav>
        <%- include ('../partials/nav.ejs') %>
      </nav>
      <div class="container">
        <div class="show">
          <img src="<%=post.img;%>" alt="car"><br/>
          <h1><%=post.name;%>'s <%=post.car%>.</h1>
          <p><%=post.description;%></p><br/>
          <img src="<%=post.img2;%>" alt="">
          <img src="<%=post.img3;%>" alt="">
        </div>
        <button type="button"><a href="/toolow/<%=post.id;%>/edit" id="edit">edit</a></button>
        <form action="/toolow/<%=post.id%>?_method=DELETE" method="POST">
          <input type="submit" value="DELETE" class="delete"/>
        </form>
      </div>
    </body>
</html>
